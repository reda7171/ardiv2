
# BEGIN WP MANAGER
	# Règles ajoutées par LWS Wordpress Manager, ne pas éditer à la main
	# Rules added by LWS Wordpress Manager, do not edit by hand
	Options +Indexes
RewriteRule ^author/(.+) "-" [F]
RewriteRule (.+)\.sql$ "-" [F]
RewriteRule (license\.txt|readme\.html)$ "-" [F]
RewriteRule xmlrpc\.php$ "-" [F]
<If "%{REQUEST_URI} =~ m#wp-content/uploads/.+\.php#">
    SetHandler !
</If>
# END WP MANAGER


# BEGIN WordPress
# Les directives (lignes) entre « BEGIN WordPress » et « END WordPress » sont générées
# dynamiquement, et doivent être modifiées uniquement via les filtres WordPress.
# Toute modification des directives situées entre ces marqueurs sera surchargée.
RewriteCond %{REQUEST_FILENAME} -s
RewriteCond %{HTTP_USER_AGENT} !facebookexternalhit/[0-9]
RewriteCond %{HTTP_USER_AGENT} !Twitterbot/[0-9]
RewriteCond %{HTTP_USER_AGENT} !Googlebot/[0-9]
RewriteRule wp-content/uploads/wp-import-export-lite(\/[A-Za-z0-9_@.\/&+-]+)+\.([A-Za-z0-9_@.\/&+-]+)$ [L]
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]
RewriteBase /ardiv2/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /ardiv2/index.php [L]
</IfModule>


# END WordPress

